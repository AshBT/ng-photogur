%a.btn{:href => "/pictures/new"} New Picture

.picture{:"ng-repeat" => "picture in pictures"}
  .row
    .span12
      %h2 {{picture.title}}
      %a.pull-right{:href => "/pictures/{{$index}}/edit"} Edit
  .row
    .span12
      %a{:href => "/pictures/{{$index}}"}
        %img{:ng-src => "{{picture.url}}"}
